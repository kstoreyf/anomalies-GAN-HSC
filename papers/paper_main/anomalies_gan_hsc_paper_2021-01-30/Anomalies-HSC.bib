author = {Kojima, Takashi and Ouchi, Masami and Rauch, Michael and Ono, Yoshiaki and Nakajima, Kimihiko and Isobe, Yuki and Fujimoto, Seiji and Harikane, Yuichi and Hashimoto, Takuya and Hayashi, Masao and Komiyama, Yutaka and Kusakabe, Haruka and Kim, Ji Hoon and Lee, Chien-Hsiu and Mukae, Shiro and Nagao, Tohru and Onodera, Masato and Shibuya, Takatoshi and Sugahara, Yuma and Umemura, Masayuki and Yabe, Kiyoto},
doi = {https://doi.org/10.3847/1538-4357/aba047},
eprint = {1910.08559},
file = {:Users/ksf/nyu/physics/mendeley{\_}files/Kojima et al. - 2019 - Extremely Metal-Poor Representatives Explored by the Subaru Survey (EMPRESS). I. A Successful Machine Learning Se.pdf:pdf},
