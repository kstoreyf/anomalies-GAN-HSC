journal = {The Astrophysical Journal},
number = {2},
title = {{SURVEY OF GRAVITATIONALLY-LENSED OBJECTS IN HSC IMAGING (SUGOHI). II. ENVIRONMENTS AND LINE-OF-SIGHT STRUCTURE OF STRONG GRAVITATIONAL LENS GALAXIES TO Z ∼ 0.8}},
